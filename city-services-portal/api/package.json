{
  "name": "city-services-portal-api",
  "version": "1.0.0",
  "description": "City Services Portal API - Municipal service request management system",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
